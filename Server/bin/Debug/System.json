{
  "ProductVersion": "2.0.1",
  "Environment": "windows",
  "EnvironmentSeparator": "\\",
  "DocumentationUrl": "http://www.komodosearch.com/docs/",
  "EnableConsole": 1,
  "Server": {
    "ListenerHostname": "+",
    "ListenerPort": 9090,
    "Ssl": 0,
    "HeaderApiKey": "x-api-key",
    "HeaderEmail": "x-email",
    "HeaderPassword": "x-password",
    "HeaderVersion": "x-version",
    "AdminApiKey": "komodoadmin"
  },
  "Files": {
    "UserMaster": "D:\\Code\\Komodo\\Core\\Server\\bin\\Debug\\UserMaster.json",
    "ApiKey": "D:\\Code\\Komodo\\Core\\Server\\bin\\Debug\\ApiKey.json",
    "ApiKeyPermission": "D:\\Code\\Komodo\\Core\\Server\\bin\\Debug\\ApiKeyPermission.json",
    "IndicesDatabase": "D:\\Code\\Komodo\\Core\\Server\\bin\\Debug\\Indices.db"
  },
  "Logging": {
    "SyslogServerIp": "127.0.0.1",
    "SyslogServerPort": 514,
    "Header": "komodo",
    "MinimumLevel": 1,
    "LogHttpRequests": 0,
    "LogHttpResponses": 0,
    "ConsoleLogging": 1
  },
  "Debug": {
    "Database": 1
  },
  "Rest": {
    "UseWebProxy": 0,
    "AcceptInvalidCerts": 0
  },
  "Indexer": {
    "IndexerIntervalMs": 5000
  }
}
